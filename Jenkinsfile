pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building..'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage ('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
        stage ('Re Deploy') {
            steps {
                echo 'Re Deploying....'
            }
        }
    }    
}